<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Web Test Suite">
    <test name="Smoke">
        <parameter name="env" value="${env}" />
        <classes>
            <class name="com.cliff.runner.RunSmokeTest" />
        </classes>
    </test>
    <test name="Regression">
        <parameter name="env" value="${env}" />
        <classes>
            <class name="com.cliff.runner.RunRegressionTest"/>
        </classes>
    </test>
</suite>
